{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "styles/**", "use": "@vercel/static" },
    { "src": "scripts/**", "use": "@vercel/static" },
    { "src": "images/**", "use": "@vercel/static" },
    { "src": "data/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "index.html" },
    { "src": "/checkout", "dest": "checkout.html" },
    { "src": "/orders", "dest": "orders.html" },
    { "src": "/tracking", "dest": "tracking.html" }
  ]
}
